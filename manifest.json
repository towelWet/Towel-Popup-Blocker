{
  "manifest_version": 2,
  "name": "Popup Blocker",
  "version": "1.0",
  "description": "A simple popup blocker extension",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
